
  true(then)(else) {
    then
  }

  false(then)(else) {
    else
  }


  equal(a)(b) {
    a(b)(b(false)(true))
  }

  not(bool) {
    bool(false)(true)
  }

  and(a)(b) {
    a(b)(false)
  }

  or(a)(b) {
    a(true)(b)
  }

  xor(a)(b) {
    a(b(false)(true))(b)
  }

  format(bool) {
    bool("true")("false")
  }

  negate(predicate)(item) {
    not(predicate(item))
  }
