
  identity(x) {
    x
  }

  constant(value)(argument) {
    value
  }

  compose(f)(g)(x) {
    f(g(x))
  }

  flip(f)(x)(y) {
    f(y)(x)
  }

  if(condition)(function)(argument) {
    condition(argument)
      (function(argument))
      (argument)
  }

  doubly(g)(f)(x)(y) {
    g(g(flip(f))(y))(x)
  }
