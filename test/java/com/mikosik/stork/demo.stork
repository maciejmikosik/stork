
  build {
    module
      (importAs("single")("stork.stream.single"))
      (importAs("array")("stork.stream.array"))
      (importAs("append")("stork.stream.append"))
      (importAs("each")("stork.stream.each"))
      (importAs("flatten")("stork.stream.flatten"))
  }

  unlines(lines) {
    flatten(
      each
        (append(single(13)))
        (lines)
    )
  }

  text {
    array(3)
      ("Demo program")
      ("")
      ("Hello world!")
  }

  main {
    unlines(text)
  }
