
  build {
    module
      (import("stork.stream.single"))
      (import("stork.stream.array"))
      (import("stork.stream.append"))
      (import("stork.stream.each"))
      (import("stork.stream.flatten"))
  }

  unlines(lines) {
    flatten(
      each
        (append(single(13)))
        (lines)
    )
  }

  text {
    array(3)
      ("Demo program")
      ("")
      ("Hello world!")
  }

  main(stdin) {
    unlines(text)
  }
