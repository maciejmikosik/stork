
  build {
    module
      (exportAs("stork.boolean.true")("true"))
      (exportAs("stork.boolean.false")("false"))
      (exportAs("stork.boolean.not")("not"))
      (exportAs("stork.boolean.and")("and"))
      (exportAs("stork.boolean.or")("or"))
  }

  true(then)(else) {
    then
  }

  false(then)(else) {
    else
  }



  not(bool) {
    bool(false)(true)
  }

  and(a)(b) {
    a(b)(false)
  }

  or(a)(b) {
    a(true)(b)
  }
