
  test {
    suite("integer")
      (suite("add")
        (case(add(2)(3))(5))
        (case(add(-1)(1))(0))
        (case(add(-10)(-5))(-15))
        (case(add(add(1)(2))(add(3)(4)))(10)))
      (suite("negate")
        (case(negate(0))(0))
        (case(negate(5))(-5))
        (case(negate(-3))(3))
        (case(negate(negate(5)))(5)))
      (suite("equal")
        (case(equal(0)(0))(true))
        (case(equal(1)(1))(true))
        (case(equal(-1)(-1))(true))
        (case(equal(0)(1))(false))
        (case(equal(1)(0))(false))
        (case(equal(1)(-1))(false))
        (case(equal(-1)(1))(false))
        (case(equal(negate(1))(negate(2)))(false)))
      (suite("moreThan")
        (case(moreThan(0)(1))(true))
        (case(moreThan(1)(0))(false))
        (case(moreThan(0)(0))(false))
        (case(moreThan(-1)(1))(true))
        (case(moreThan(1)(-1))(false))
        (case(moreThan(negate(2))(negate(3)))(false)))
  }
