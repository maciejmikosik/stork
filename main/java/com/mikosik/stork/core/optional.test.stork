
  build {
    module
      (import("stork.optional.present"))
      (import("stork.optional.absent"))
      (import("stork.optional.else"))
  }

  test {
    suite("optional")
      (suite("present/absent")
        (case(present(x)(f)(g))(f(x)))
        (case(absent(f)(g))(g)))
      (suite("else")
        (case(else(y)(present(x)))(x))
        (case(else(y)(absent))(y)))
  }
