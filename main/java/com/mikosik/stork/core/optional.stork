
  build {
    module
      (exportAs("stork.optional.present")("present"))
      (exportAs("stork.optional.absent")("absent"))
      (exportAs("stork.optional.else")("else"))

      (importAs("self")("stork.function.self"))
  }

  present(element)(vPresent)(vAbsent) {
    vPresent(element)
  }

  absent(vPresent)(vAbsent) {
    vAbsent
  }



  else(default)(optional) {
    optional
      (self)
      (default)
  }
