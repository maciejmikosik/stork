
  build {
    module
      (importAs("true")("stork.boolean.true"))
      (importAs("false")("stork.boolean.false"))
      (importAs("not")("stork.boolean.not"))
      (importAs("and")("stork.boolean.and"))
      (importAs("or")("stork.boolean.or"))
  }

  test {
    suite("boolean")
      (suite("true/false")
        (case(true(x)(y))(x))
        (case(false(x)(y))(y)))
      (suite("not")
        (case(not(true))(false))
        (case(not(false))(true)))
      (suite("and")
        (case(and(true)(true))(true))
        (case(and(false)(true))(false))
        (case(and(true)(false))(false))
        (case(and(false)(false))(false)))
      (suite("or")
        (case(or(true)(true))(true))
        (case(or(false)(true))(true))
        (case(or(true)(false))(true))
        (case(or(false)(false))(false)))
  }
