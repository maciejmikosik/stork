
  build {
    module
      (import("stork.function.self"))
      (import("stork.function.compose"))
      (import("stork.function.flip"))
      (import("stork.opcode.opArg"))
  }

  test {
    suite("function")
      (suite("self")
        (case
          (self(x))
          (x)))
      (suite("compose")
        (case
          (compose(f)(g)(x))
          (f(g(x))))
        (case
          (compose(opArg(f))(compose(g)(h))(x))
          (f(g(h(x)))))
        (case
          (compose(compose(f)(g))(h)(x))
          (f(g(h(x)))))
        (case
          (compose(compose(opArg(f))(opArg(g)))(compose(h)(i))(x))
          (f(g(h(i(x)))))))
      (suite("flip")
        (case
          (flip(f)(x)(y))
          (f(y)(x)))
        (case
          (flip(f)(x)(y)(z))
          (f(y)(x)(z))))
  }
